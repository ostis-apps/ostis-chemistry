      [Утв. (геохимия)](* <- lang_ru;; *);
      [Stat. (geochemistry)](* <- lang_en;; *);;
    <= nrel_sc_text_translation:...
      (*
        -> rrel_example: [В задачи геохимии входят: определение относительной и абсолютной распространённости элементов и изотопов в Земле и на её поверхности; изучение распределения и перемещения элементов в различных частях Земли (коре, мантии, гидросфере и т. д.) для выяснения законов и причин неравномерного распределения элементов и другие.](* <- lang_ru;; => nrel_format: format_html;; *);;
      *);;
  *);

